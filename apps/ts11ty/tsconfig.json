{
  "compilerOptions": {
    "outDir": "./_site",
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "jsxImportSource": "jsx-async-runtime"
  },
  "extends": ["@tsconfig/node-lts/tsconfig.json"],
  "include": ["src/**/*.ts", "src/**/*.tsx", "test/**/*.ts", "test/**/*.tsx"]
}
